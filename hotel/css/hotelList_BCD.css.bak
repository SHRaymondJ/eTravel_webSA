.mapBody{
	position: fixed;
	width: 600px;
	height: 625px;
	top:50%;
    left:50%;
    transform:translate(-50%,-50%);
	z-index: 100;
	background-color: #fff;
}
#mapTittle{
	width: 100%;
	height: 25px;
	text-align: right;
	font-size: 25px;
	line-height: 25px;
}
.mapTittleText{
	margin-right: 10px;
	color: #4a4a4a;
	cursor: pointer;
}
.mapTittleText:hover{
	color:#041e58;
}
#map{
	width: 600px;
	height: 600px;
}
/*进度条*/
.progressBar{
	width: 100%;
	height: 50px;
	line-height: 50px;
	font-size: 14px;
	color: #9b9b9b;
}
.progressLine{
	width: 304px;
	height: 2px;
	/*background-color: #041e58;*/
	margin:24px 0 0 30px;
}
.progressCircle{
	width: 6px;
    height: 6px;
    /*background-color: #041e58;*/
    margin: 22px 10px 0 0;
    border-radius: 50%;
}
.hotelsNumber{
	width: 100%;
	height: 60px;
	line-height: 60px;
	font-size: 18px;
	box-sizing: border-box;
	padding-left: 20px;
	/*color: #041e58;*/
	border-bottom: 1px solid #a9a9a9;
	position: relative;
}
.allMapBtn{
	/* 2020-4-10 */
	display: inline-block;
	width: 100px;
	height: 30px;
	color: #fff;
	/*background-color: #f6aa25;*/
	text-align: center;
	line-height: 30px;
	margin-top: 15px;
	border-radius: 10px;
	font-size: 14px;
	cursor: pointer;
	position: absolute;
	left: 1015px;
}
/*搜索框*/
.searchBody{
	width: 100%;
	height: 150px;
	background-color: #fff;
	position: relative;
	top: 10px;
	margin-bottom: 14px;
}
.searchConditionBody{
	background-color:#e9f0f6;
	border-top:1px solid #041e5b;
}
#hotelCity,#hotelIntlCity,#hotelDepartureDate,#hotelReturnDate,#keyWordInput,.hotelAddressBody{
	width: 125px;
	height: 26px;
	margin-top: 11px;
	border: 1px solid #979797;
	position: relative;
}
.hotelAddressBody{
	background: #fff;
}
#hotelAddress{
	width: 100px;
	height: 26px;
	border: 0;
	position: absolute;
	top: 0;
	left: 0;
	margin: 0;
	z-index: 10;
}
#hotelAddressSelect{
	width: 120px;
	height: 26px;
	border: 0;
	position: absolute;
	top: 0;
	left: 0;
	margin: 0;
	z-index: 1;
	outline:none;
}
#keyWordInput{
	width: 200px;
}
.keyWordBody{
	width: 408px;
	position: absolute;
	top: 40px;
	left: 790px;
	background-color: #fff;
	z-index: 99;
	 display: none; 
	border: 1px solid #F6AA25;
	padding-bottom: 15px;
}
.relationTittle{
	height: 35px;
	line-height: 35px;
	font-size: 14px;
	font-weight: bold;
	box-sizing: border-box;
	padding-left: 10px;
}
.relationLi{
	width: 24%;
	height: 35px;
	line-height: 35px;
	font-size: 13px;
	text-align: center;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	border: 1px solid #f3f3f3;
	cursor: pointer;
}
.keyWordBodyLi{
	text-align: center;
	width: 100%;
	height: 30px;
	line-height: 30px;
	border-bottom: 1px solid #c2c2c2;
	cursor: pointer;
}
.hotelStarBtn{
	width:150px;
	height:26px;
	border: 1px solid #979797;
	margin:11px 0 0 15px;
	line-height: 26px;
	text-align: center;
	cursor: pointer;
}
.starChoose{
/*	background-color: #041e58;
	color: #fff;*/
	border: 1px solid #041e58;
    color: #fff;
    background: #041e5b;
}
.hotelPriceBtn,.searchMinPrice,.searchMaxPrice{
	width:80px;
	height:26px;
	border: 1px solid #979797;
	margin:11px 0 0 15px;
	line-height: 26px;
	text-align: center;
	cursor: pointer;
}
.searchMinPrice{
	margin: 11px 10px 0 30px;
}
.searchMaxPrice{
	margin-left:10px;
}
.searchHotelBtn{
	width: 80px;
	height: 30px;
	line-height: 30px;
	/*background-color: #f6aa25;*/
	text-align: center;
	border-radius: 8px;
	margin: 10px 0 0 45px;
	cursor: pointer;
	color: #fff;
}
/*信息界面*/
.infoBody{
	background-color: #fff;
	width: 100%;
}
.hotelChooseBody{
	height: 40px;
	line-height: 40px;
	font-size: 14px;
	border-top: 4px solid #f1f1f1;
	box-sizing: border-box;
	padding-left: 20px;
	/* color: #DC002E; */
	color: #f6aa25;
	
}
/*筛选部分*/
.siftBody{
	width: 100%;
	height:52px;
	line-height: 52px;
	background-color: #e9f0f6;
	border: #CCC 1px solid;
	position: relative;
	font-size: 13px;
}
.priceSortIcon,.starSortIcon,.scoreSortIcon,.distanceSortIcon{
	width:11px;
	height: 13px;
	margin: 18px 0 0 5px;
	background-image: url('../../css/images/sortIcon.png');
	background-position:0px 0px;
}
.breakfastBtn,.freeCancelBtn,.noGuaranteeBtn,.protocolBtn{
	width: 115px;
	height: 25px;
	line-height: 25px;
	border: 1px solid #979797;
	margin: 13px 0 0 20px;
	border-radius: 3px;
	text-align: center;
	background-color: #fff;
	cursor: pointer;
}
/*酒店列表*/
.hotelList{
	margin-top: 5px;
}
.hotelList .hotelLi{
	width: 100%;
	min-height: 275px;
	border-bottom: 1px solid #a9a9a9;
	position: relative;
}
.hotelLiImg{
	width: 200px;
	height: 200px;
	position: absolute;
	left: 20px;
	top: 38px;
	background-size: 100% 100%;
	background-repeat: no-repeat;
}
.hotelLiName{
	min-width: 200px;
	height: 20px;
	line-height: 20px;
	font-size: 16px;
	font-weight: bold;
	/*color: #041e58;*/
	position: absolute;
	top: 38px;
	left: 250px;
	cursor: pointer;
}
.hotelLiScore{
	width: 30px;
	height: 30px;
	/*background-color: #f6aa25;*/
	border-radius: 0px 10px 0px 10px;;
	text-align: center;
	line-height: 30px;
	position: absolute;
	top: 35px;
	right: 100px;
}
.hotelLiGrade{
	width: 100px;
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	color: #041e58;
	text-align: right;
	position: absolute;
	top: 35px;
    right: 135px;
}
.hotelLiComment{
	width: 100px;
	height: 20px;
	line-height: 20px;
	font-size: 12px;
	color: #404040;
	text-align: right;
	position: absolute;
	top: 49px;
    right: 135px;
}
.hotelLiLine{
	width: 920px;
	height: 0px;
	border-bottom: 1px dashed #979797;
	position: absolute;
	left: 250px;
	top: 70px;
}
.hotelLiInfo{
	width: 600px;
	height: 170px;
	position: absolute;
	left: 250px;
	top: 85px;
}
.hotelLiDistance{
	min-width: 200px;
	height: 40px;
	line-height: 40px;
	font-size: 12px;
}
.hotelLiStar{
	height: 16px;
	background-image: url("../images/starIcon.png");
	
	position: relative;
}
/* 12.23新增 */
.hotelLiStarHalf{
	position: absolute;
	height: 16px;
	width: 16px;
	right: -16px;
	/* background-color: rgba(100,120,12,0.3); */
	background-image: url("../images/halfhotstar.png");
	background-repeat: no-repeat;
}
.hotelLiAddress{
	min-width: 200px;
	max-width: 600px;
	height: 40px;
	line-height: 40px;
	font-size: 12px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.mapIcon{
    width: 20px;
    height: 20px;
    margin-top: 10px;
    background-image: url(../../hotel/images/mapIcon.png);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    cursor: pointer;
}
.hotelLiTelephone{
	min-width: 200px;
	height: 40px;
	line-height: 40px;
	font-size: 12px;
}
.hotelLiPrice{
	/* 2020-01-02 修改*/
	/* width: 200px; */
	min-width: 120px;
	top: 120px;
	/* right: 100px; */
	right: 70px;
	font-size: 12px;
	position: absolute;
	/* text-align: right; */
	text-align: center;
	color: #041e58;
}
.searchHotelDetailBtn{
	width: 120px;
	height: 35px;
	line-height: 35px;
	font-size: 14px;
	/*background-color: #041e58;*/
	color: #fff;
	text-align: center;
	border-radius: 5px;
	position: absolute;
	right: 70px;
	bottom: 40px;
	cursor: pointer;
}
.protocolBody{
	/* width: 60px; */
	width: 90px;
    height: 40px;
    position: absolute;
    /* right: -60px; */
    right: -90px;
    top: 20px;
}
.protocolText{
	width: 100%;
	height: 25px;
	line-height: 25px;
	font-size: 13px;
	background-color: #8dc73f;
	color:#fff;
	text-align: center;
}
.triangleTopRight{
	width: 0;
    height: 0;
    border-top: 7px solid #446a10;
    /* border-right: 60px solid transparent; */
    border-right: 90px solid transparent;
}
.protocolTextOrg{
	/* background-color: #FF9B3F; */
	background-color: #5ABA2C;
}
.triangleTopRightOrg{
    /* border-top: 7px solid #ED8322; */
    border-top: 7px solid #459114;
}
/* 新增图片 */
.picBody{
	position: relative;
	
}
.picHref{
	    position: absolute;
	    height: 100%;
	    width: 100%;
	    left: 0;
	    top: 0;
		cursor: pointer;
}